{
  "name": "@dsh/backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "dev": "nodemon -w tools -e ts --exec \"ts-node-transpile-only -r tsconfig-paths/register -r source-map-support/register src --watch\"",
    "build": "tsoa spec-and-routes --configuration src/servers/rest/tsoa.json && tsc && tsc-alias",
    "start": "node src"
  },
  "engines": {
    "node": "18",
    "pnpm": "8"
  },
  "dependencies": {
    "arangojs": "8.4.0",
    "koa": "2.14.2",
    "ms": "2.1.3",
    "tsoa": "5.1.1",
    "tsyringe": "4.8.0",
    "vality": "6.3.3",
    "vality-env": "1.0.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.74",
    "@swc/helpers": "0.5.1",
    "@swc/jest": "0.2.27",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/jest": "29.5.3",
    "@types/koa": "2.13.8",
    "@types/ms": "0.7.31",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.6.2",
    "nodemon": "3.0.1",
    "prettier": "3.0.1",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "@types/node"
      ]
    }
  }
}
